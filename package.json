{
  "name": "@u3u/vue-hooks",
  "description": "⚡️ Awesome Vue Hooks",
  "version": "1.0.1-alpha",
  "keywords": [
    "vue",
    "vue-use",
    "vue-hooks",
    "vue-function-api"
  ],
  "main": "lib/index.js",
  "module": "esm/index.js",
  "files": [
    "lib/",
    "esm/",
    "types/"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build:cjs": "tsc",
    "build:es": "tsc -m esNext --outDir esm",
    "build": "yarn clean && yarn build:cjs && yarn build:es",
    "clean": "rimraf lib esm",
    "lint": "eslint 'src/**/*.ts'",
    "lint:types": "tsc --noEmit",
    "lint:prettier": "prettier-check '**/*.{ts,md,mdx}'",
    "format": "prettier '**/*.{ts,md,mdx}' --write",
    "prepublishOnly": "yarn test && yarn build"
  },
  "author": {
    "name": "u3u",
    "email": "qwq@qwq.cat",
    "url": "https://qwq.cat"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/u3u/vue-hooks"
  },
  "bugs": {
    "url": "https://github.com/u3u/vue-hooks/issues"
  },
  "homepage": "https://github.com/u3u/vue-hooks#readme",
  "engines": {
    "node": ">=10.16.0"
  },
  "dependencies": {
    "dayjs": "^1.8.15",
    "vue": "^2.6.10",
    "vue-function-api": "^2.0.6",
    "vue-router": "^3.0.7",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-typescript": "^7.3.3",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.2",
    "@typescript-eslint/parser": "^1.12.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "all-contributors-cli": "^6.7.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2",
    "prettier-check": "^2.0.0",
    "rimraf": "^2.6.3",
    "typescript": "^3.5.3",
    "vue-template-compiler": "^2.6.10",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E GIT_PARAMS"
  },
  "lint-staged": {
    "*.ts": [
      "eslint"
    ],
    "*.{ts,md,mdx}": "prettier-check"
  }
}
